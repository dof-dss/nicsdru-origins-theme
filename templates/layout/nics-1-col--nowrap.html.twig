{#
/**
 * @file
 * Theme implementation to display a one-column layout with no wrappers.
 *
 * Available variables:
 * - content: The content for this layout.
 * - attributes: HTML attributes for the layout <div>.
 *
 * @ingroup themeable
 */
#}
{% if content %}
  {% if is_admin %}
    <div {{ region_attributes.main }}>
  {% endif %}
    {% if content.main %}
      {{- content.main -}}
    {% endif %}
  {% if is_admin %}
    </div>
  {% endif %}
{% endif %}
